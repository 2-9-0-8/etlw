---
import Layout from '@/layouts/layout.astro'
import ContentWrapper from '@/components/content-wrapper.astro'
import Heading from '@/components/heading.astro'
import YSection from '@/components/y-section.astro'

export const prerender = true
---

<Layout title="About">
  <div class="bg-b1ack-textured text-off-white">
    <ContentWrapper>
      <YSection size="md" class="text-center">
        <Heading level={1}>404: Page missing!</Heading>
        <p class="font-medium text-lg">Sorry, the page you are looking for does not exist.</p>
      </YSection>
    </ContentWrapper>
  </div>
</Layout>
